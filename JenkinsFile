pipeline {
    agent any
    stages {
        stage('Pipeline Stages'){
            steps {
                sh "ls"
            }
        }
        stage('second stage'){
            steps {
                sh "pwd"
            }
        }
        stage('Touch stage'){
            steps {
                sh "touch "example.txt""
            }
        }
        stage('Echo stage'){
            steps {
                sh "echo "Oi Oi""
            }
        }
        stage('Cat stage'){
            steps {
               sh "cat "example.txt""
            }
        }
        stage('Make stage'){
            steps {
               sh "mkdir "examplestore""
            }
        }
        stage('Move Stage'){
            steps {
               sh "mv "example.txt examplestore/example.txt""
            }
        }
        stage('Change Stage'){
            steps {
               sh "cd "examplestore""
            }
        }
        stage('Test'){
            steps {
               sh "cat "example.txt""
            }
        }
    }
}