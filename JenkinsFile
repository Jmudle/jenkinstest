pipeline {
    agent any
    stages {
        stage('Pipeline Stages') {
            steps {
                sh "ls"
            }
        }
        stage('Second stage') {
            steps {
                sh "pwd"
            }
        }
        stage('Touch stage') {
            steps {
                sh 'touch example.txt'
            }
        }
        stage('Echo stage') {
            steps {
                sh 'echo "Oi Oi" > "example.txt"'
            }
        }
        stage('Cat stage') {
            steps {
                sh 'cat example.txt'
            }
        }
        stage('Make stage') {
            steps {
                sh 'mkdir examplestore'
            }
        }
        stage('Move Stage') {
            steps {
                sh 'mv example.txt examplestore/example.txt'
            }
        }
        stage('Change Stage') {
            steps {
                dir('examplestore') {
                    sh 'pwd'
                }
            }
        }
        stage('Test') {
            steps {
                sh 'cat examplestore/example.txt'
            }
        }
    }
}
