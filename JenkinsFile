pipeline {
    agent any
    stages {
        stage('Pipeline Stages'){
            steps {
                sh "ls"
            }
        }
        stage('second stage'){
            steps {
                sh "pwd"
            }
        }
        stage('Touch stage'){
            steps {
                touch "example.txt"
            }
        }
        stage('Echo stage'){
            steps {
                echo "Oi Oi"
            }
        }
        stage('Cat stage'){
            steps {
                cat "example.txt"
            }
        }
        stage('Make stage'){
            steps {
                mkdir "examplestore"
            }
        }
        stage('Move Stage'){
            steps {
                mv "example.txt examplestore/example.txt"
            }
        }
        stage('Change Stage'){
            steps {
                cd "examplestore"
            }
        }
        stage('Test'){
            steps {
                cat "example.txt"
            }
        }
    }
}